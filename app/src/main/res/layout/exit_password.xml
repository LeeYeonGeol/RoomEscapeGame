<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dip"
    android:layout_height="450dip">


    <Button
        android:id="@+id/st2_button"
        android:layout_width="wrap_content"
        android:layout_height="79dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="104dp"
        android:layout_marginBottom="101dp"
        android:onClick="st2_password"
        android:text="1"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/st2_button5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/st2_button2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="8dp"
        android:onClick="st2_password"
        android:text="2"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/st2_button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toEndOf="@+id/st2_button"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/st2_button3"
        android:layout_width="84dp"
        android:layout_height="0dp"
        android:layout_marginTop="104dp"
        android:onClick="st2_password"
        android:text="3"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/st2_button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/st2_button2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/st2_button4"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="2dp"
        android:onClick="st2_password"
        android:text="4"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/st2_button7"
        app:layout_constraintEnd_toStartOf="@+id/st2_button5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/st2_button" />

    <Button
        android:id="@+id/st2_button5"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="2dp"
        android:onClick="st2_password"
        android:text="5"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/st2_button8"
        app:layout_constraintEnd_toStartOf="@+id/st2_button6"
        app:layout_constraintStart_toEndOf="@+id/st2_button4"
        app:layout_constraintTop_toBottomOf="@+id/st2_button2" />

    <Button
        android:id="@+id/st2_button6"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:onClick="st2_password"
        android:text="6"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/st2_button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/st2_button5"
        app:layout_constraintTop_toBottomOf="@+id/st2_button3" />

    <Button
        android:id="@+id/st2_button7"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="103dp"
        android:onClick="st2_password"
        android:text="7"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/st2_button8"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/st2_button4" />

    <Button
        android:id="@+id/st2_button8"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="103dp"
        android:onClick="st2_password"
        android:text="8"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/st2_button9"
        app:layout_constraintStart_toEndOf="@+id/st2_button7"
        app:layout_constraintTop_toBottomOf="@+id/st2_button5" />

    <Button
        android:id="@+id/st2_button9"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="103dp"
        android:onClick="st2_password"
        android:text="9"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/st2_button8"
        app:layout_constraintTop_toBottomOf="@+id/st2_button6" />

    <TextView
        android:id="@+id/st2_exit_puzzle_title"
        android:layout_width="247dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="비밀번호를 입력하세요."
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.085" />

    <TextView
        android:id="@+id/st2_exit_puzzle_status"
        android:layout_width="217dp"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/st2_password_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="51dp"
        android:onClick="st2_password"
        android:text="입력"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/st2_button"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>